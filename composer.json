{
  "name": "ifcastle/aql-generator",
  "description": "* ## Generator interface",
  "keywords": [
    "aql",
    "query",
    "language",
    "storage"
  ],
  "type": "ifcastle-aql-package",
  "license": "MIT",
  "authors": [
    {
      "name": "Edmond Dantes",
      "email": "edmondifthen@proton.me"
    }
  ],
  "require": {
    "php": ">=8.4",
    "ifcastle/aql-dsl": "^0.1.0",
    "ifcastle/aql-storage": "^0.1.0",
    "ifcastle/di": "^0.6.0"
  },
  "autoload": {
    "psr-4": {
      "IfCastle\\AQL\\Generator\\": [
        "src/"
      ]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "IfCastle\\AQL\\Generator\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^11.2",
    "rector/rector": "^1.2"
  },
  "scripts": {
    "test": "vendor/bin/phpunit ./tests"
  }
